<?xml version="1.0" encoding="utf-8"?>
<layout  xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <data>
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/page_bk">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:background="@drawable/primary_gradient"
                android:layout_height="80dp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/imgBack"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginLeft="15dp"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/back"/>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_height="wrap_content"
                    android:text="Role  Interest"
                    android:textSize="27dp"
                    android:fontFamily="@font/sf_500"
                    android:textColor="@color/white"/>
                <View
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginRight="15dp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="21dp"
                android:background="@color/page_bk"
                android:orientation="horizontal">
                <androidx.core.widget.NestedScrollView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/bk_page_white"
                    android:scrollbars="none">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginHorizontal="18dp"
                            android:layout_marginTop="20dp"
                            android:orientation="horizontal">
                            <ImageView
                                android:layout_width="31dp"
                                android:layout_height="31dp"
                                android:gravity="center_vertical"
                                app:tint="@color/black"
                                android:layout_gravity="center_vertical"
                                android:src="@drawable/ic_interest"/>
                            <TextView
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:layout_height="wrap_content"
                                android:text="Role Interest"
                                android:textSize="20dp"
                                android:gravity="center_vertical"
                                android:layout_gravity="center_vertical"
                                android:fontFamily="@font/sf_600"
                                android:textColor="#4B5660"
                                android:layout_marginLeft="10dp"/>
                            <LinearLayout
                                android:id="@+id/edit"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">                         <ImageView
                                android:layout_width="15dp"
                                android:layout_height="15dp"
                                android:gravity="center_vertical"
                                android:layout_gravity="center_vertical"
                                android:src="@drawable/ic_edit"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Edit"
                                    android:textSize="17dp"
                                    android:gravity="center_vertical"
                                    android:layout_gravity="center_vertical"
                                    android:fontFamily="@font/sf_600"
                                    android:textColor="#8A4999"
                                    android:layout_marginLeft="10dp"/>
                            </LinearLayout>
                        </LinearLayout>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="2dp"
                            android:layout_marginTop="10dp"
                            android:layout_marginHorizontal="16dp"
                            android:background="#D2DCE2"/>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:visibility="visible"
                            android:orientation="vertical">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Are you interested in serving as a preceptor for
a graduate nurse?"
                                android:layout_marginLeft="18dp"
                                android:layout_marginTop="28dp"
                                android:textSize="15dp"
                                android:textColor="#848297"
                                android:fontFamily="@font/sf_500"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Yes"
                                android:layout_marginLeft="35dp"
                                android:layout_marginTop="7dp"
                                android:textSize="16dp"
                                android:textColor="@color/black"
                                android:fontFamily="@font/sf_500"/>
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_marginTop="6dp"
                                android:layout_marginHorizontal="16dp"
                                android:background="#D2DCE2"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Are you interested in serving as an interim nurse leader?"
                                android:layout_marginLeft="18dp"
                                android:layout_marginTop="20dp"
                                android:textSize="15dp"
                                android:textColor="#848297"
                                android:fontFamily="@font/sf_500"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="No"
                                android:layout_marginLeft="35dp"
                                android:layout_marginTop="7dp"
                                android:textSize="16dp"
                                android:textColor="@color/black"
                                android:fontFamily="@font/sf_500"/>
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_marginTop="6dp"
                                android:layout_marginHorizontal="16dp"
                                android:background="#D2DCE2"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Clinical educator"
                                android:layout_marginLeft="18dp"
                                android:layout_marginTop="20dp"
                                android:textSize="15dp"
                                android:textColor="#848297"
                                android:fontFamily="@font/sf_500"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="No"
                                android:layout_marginLeft="35dp"
                                android:layout_marginTop="7dp"
                                android:textSize="16dp"
                                android:textColor="@color/black"
                                android:fontFamily="@font/sf_500"/>
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_marginTop="6dp"
                                android:layout_marginHorizontal="16dp"
                                android:background="#D2DCE2"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Daisy Award winner?"
                                android:layout_marginLeft="18dp"
                                android:layout_marginTop="20dp"
                                android:textSize="15dp"
                                android:textColor="#848297"
                                android:fontFamily="@font/sf_500"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="No"
                                android:layout_marginLeft="35dp"
                                android:layout_marginTop="7dp"
                                android:textSize="16dp"
                                android:textColor="@color/black"
                                android:fontFamily="@font/sf_500"/>
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_marginTop="6dp"
                                android:layout_marginHorizontal="16dp"
                                android:background="#D2DCE2"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Employee of the mth, qtr, yr?"
                                android:layout_marginLeft="18dp"
                                android:layout_marginTop="20dp"
                                android:textSize="15dp"
                                android:textColor="#848297"
                                android:fontFamily="@font/sf_500"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="No"
                                android:layout_marginLeft="35dp"
                                android:layout_marginTop="7dp"
                                android:textSize="16dp"
                                android:textColor="@color/black"
                                android:fontFamily="@font/sf_500"/>
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_marginTop="6dp"
                                android:layout_marginHorizontal="16dp"
                                android:background="#D2DCE2"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Other nursing awards?"
                                android:layout_marginLeft="18dp"
                                android:layout_marginTop="20dp"
                                android:textSize="15dp"
                                android:textColor="#848297"
                                android:fontFamily="@font/sf_500"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="No"
                                android:layout_marginLeft="35dp"
                                android:layout_marginTop="7dp"
                                android:textSize="16dp"
                                android:textColor="@color/black"
                                android:fontFamily="@font/sf_500"/>
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_marginTop="6dp"
                                android:layout_marginHorizontal="16dp"
                                android:background="#D2DCE2"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Professional practice council?"
                                android:layout_marginLeft="18dp"
                                android:layout_marginTop="20dp"
                                android:textSize="15dp"
                                android:textColor="#848297"
                                android:fontFamily="@font/sf_500"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="No"
                                android:layout_marginLeft="35dp"
                                android:layout_marginTop="7dp"
                                android:textSize="16dp"
                                android:textColor="@color/black"
                                android:fontFamily="@font/sf_500"/>
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_marginTop="6dp"
                                android:layout_marginHorizontal="16dp"
                                android:background="#D2DCE2"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Research / publications?"
                                android:layout_marginLeft="18dp"
                                android:layout_marginTop="20dp"
                                android:textSize="15dp"
                                android:textColor="#848297"
                                android:fontFamily="@font/sf_500"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="No"
                                android:layout_marginLeft="35dp"
                                android:layout_marginTop="7dp"
                                android:textSize="16dp"
                                android:textColor="@color/black"
                                android:fontFamily="@font/sf_500"/>
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_marginTop="6dp"
                                android:layout_marginHorizontal="16dp"
                                android:background="#D2DCE2"/>
                        </LinearLayout>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Select Languages"
                            android:layout_marginLeft="18dp"
                            android:layout_marginTop="28dp"
                            android:textSize="15dp"
                            android:textColor="#848297"
                            android:fontFamily="@font/sf_500"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="English, Spanish"
                            android:layout_marginLeft="35dp"
                            android:layout_marginTop="7dp"
                            android:textSize="16dp"
                            android:textColor="@color/black"
                            android:fontFamily="@font/sf_500"/>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginTop="6dp"
                            android:layout_marginHorizontal="16dp"
                            android:background="#D2DCE2"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Introduce Yourself (Feel free to upload
additional photos)"
                            android:layout_marginLeft="18dp"
                            android:layout_marginTop="15dp"
                            android:textSize="15dp"
                            android:textColor="#848297"
                            android:fontFamily="@font/sf_500"/>
                        <TextView
                            android:visibility="visible"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="I have a Master’s Degree in nursing and 8+ years of proven experience effectively and efficiently managing both patients and staff. I am an extremely organized, calm, and patient professional with excellent healthcare skills. I have a passion for providing quality care to patients, and the necessary leadership skills to inspire other staff members to strive to provide above standard levels of service."
                            android:layout_marginLeft="20dp"
                            android:layout_marginTop="7dp"
                            android:textSize="16dp"
                            android:textColor="@color/black"
                            android:fontFamily="@font/sf_500"/>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginTop="6dp"
                            android:layout_marginHorizontal="16dp"
                            android:background="#D2DCE2"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="YouTube / Vimeo Link"
                            android:layout_marginLeft="18dp"
                            android:layout_marginTop="15dp"
                            android:textSize="15dp"
                            android:textColor="#848297"
                            android:fontFamily="@font/sf_500"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="https://vimeo.com/upgrade"
                            android:layout_marginLeft="35dp"
                            android:layout_marginTop="7dp"
                            android:textSize="16dp"
                            android:textColor="@color/black"
                            android:fontFamily="@font/sf_500"/>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginTop="6dp"
                            android:layout_marginHorizontal="16dp"
                            android:background="#D2DCE2"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Additional Photos"
                            android:layout_marginLeft="18dp"
                            android:layout_marginTop="15dp"
                            android:textSize="15dp"
                            android:textColor="#848297"
                            android:fontFamily="@font/sf_500"/>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="125dp"
                            android:layout_marginTop="16dp"
                            android:layout_marginHorizontal="20dp"
                            android:orientation="horizontal">
                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:layout_marginRight="28dp"
                                android:background="#C4C4C4">
                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_margin="4dp"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/test2"/>
                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:background="#C4C4C4">
                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_margin="4dp"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/test2"/>
                            </LinearLayout>
                        </LinearLayout>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Additional Files (e.g., Letters of Recommendation)"
                            android:layout_marginLeft="18dp"
                            android:layout_marginTop="15dp"
                            android:textSize="15dp"
                            android:textColor="#848297"
                            android:fontFamily="@font/sf_500"/>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="220dp"
                            android:layout_weight="1"
                            android:layout_marginLeft="20dp"
                            android:layout_marginTop="12dp"
                            android:layout_marginBottom="30dp"
                            android:layout_marginRight="80dp"
                            android:background="#C4C4C4">
                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="4dp"
                                android:scaleType="fitXY"
                                android:src="@drawable/test4"/>
                        </LinearLayout>
                    </LinearLayout>
                </androidx.core.widget.NestedScrollView>
            </LinearLayout>
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>